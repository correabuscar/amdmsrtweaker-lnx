#!/bin/bash

sudo=''

if [ `id -u` != 0 ] ; then
  echo "!! You're not root, using sudo..."
#    echo "you are not root! press a key, not shift/ctrl/alt"
#    read -n 1 -s
#    exit 1
  sudo='sudo -- '
fi


$sudo modprobe msr || $sudo insmod ./msr.ko && echo 'good module msr!'
#$sudo modprobe cpuid
$sudo ./amdmsrt4myZ575
$sudo rmmod msr
#$sudo rmmod cpuid

